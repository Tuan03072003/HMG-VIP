HOME = 1
function HOME()
  HM = gg.choice({
  	 "‚Ä¢  ô è·¥ò·¥Äss üÖªüÖæüÖ±üÖ±üÜà‚Ä¢",
       "‚Ä¢ ·¥è…¥/·¥è“ì“ì¬†·¥Ö·¥Ä·¥õ·¥Ä ‚Ä¢",
       "‚Ä¢  ü·¥áss¬† Ä·¥á·¥Ñ·¥è…™ ü üÖªüÖæüÖ±üÖ±üÜà",
       "‚Ä¢ ·¥Ä…¥·¥õ·¥á…¥…¥·¥Ä üÖ∂üÖ∞üÖºüÖ¥ ‚Ä¢",
       "‚Ä¢ ·¥Ä…™·¥ç ô·¥è·¥õ üÖ∂üÖ∞üÖºüÖ¥‚Ä¢",
        "„ÄêÔªøüÖ¥üÜáüÖ∏üÜÉ„Äë"
  }, nil, "·¥õ·¥á ü·¥á…¢ Ä·¥Ä·¥ç: @ ú·¥ç…¢ ú·¥Ä·¥ã·¥ã" )
